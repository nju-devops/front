{
  "defaultSeverity": "error",
  
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "no-empty-interface": true,
    "semicolon": [
      true,
      "never"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "ordered-imports": false
  }
}